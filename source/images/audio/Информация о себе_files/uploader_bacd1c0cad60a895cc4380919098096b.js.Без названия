uploader = 
{
	master_key:'asdasd',
	frames_list: Array(),
	active_uploads: Array(),
	frame_files:Array(),
	max_uploads: 10,
	max_total_size: 20,
	size: Array(),
	permitted_ext: Array('jpg','gif','png','zip','rar','gz','tar','doc','rtf','pdf','psd','ai','flv', 'txt','swf','jpeg','docx', 'xls', 'xlsx','csv','bmp','mp3','tiff','tif'),	
	uploaded_count: 0,
	total_files_count: 0,
	ready: false,
	loaded_callback: null,
	files_file_id: Array(),
	//delete_callback: null,
	error_strings: {
		'file_corrupt':'Файл поврежден. ',
		'unknown_error':'Ошибка загрузки файла. Проверьте его размер и расширение. ',
        'wrong_dimension':'Разрешение файла превышает 5000x15000 пикселей. ',
		'wrong_extension':'Недопустимое расширение. ',
		'wrong_size_max':'Превышен максимальный размер файла. ',
		'wrong_size_min':'Файл пуст. ',
		'wrong_content':'Архив содержит файлы с недопустимым раcширением. <a href="/help/118.html" target="_blank">Подробнее &rarr;</a> ',
		'no_index_file':'В архиве отсутствует файл index.html. <a href="/help/118.html" target="_blank">Подробнее &rarr;</a> ',
		'no_meta_tag':'Файл index.html не содержит meta-тега area.size. <a href="/help/118.html" target="_blank">Подробнее &rarr;</a> ',
		'no_preview_file':'В архиве отсутствет файл preview.jpg. <a href="/help/118.html" target="_blank">Подробнее &rarr;</a> ',
		'toot_many_files':'Превышено максимальное количество файлов. <a href="/help/118.html" target="_blank">Подробнее &rarr;</a>',
		'upload_error1':'Превышен максимальный размер файла. ',
		'upload_error2':'Превышен максимальный размер файла. ',
		'upload_error3':'Файл получен частично. ',
		'upload_error4':'Файл не был загружен. '
			},
	timer: null,
	pending: null,
	add_upload:function() 
	{
		var name = this.frames_list.length+1;
		name = this.master_key+name;
		this.frames_list[this.frames_list.length]=true;
		frame_ = $('<iframe class="hidden" name="'+name+'"></iframe>');
		frame_.bind('load',function(){ uploader.loaded(this); });
		this.frame_files[this.frames_list.length] = [];
		//$(frame).data('fids',fids);
		$('#upload_form').after(frame_);	
		$('#upload_form').attr('target',name);
		$('input[type=file]').attr('value','');
		$('#UPLOAD_IDENTIFIER').attr('value',name);		
		$('#upload_form').attr('action', uploader_url+'/upload/?X-Progress-ID='+name);
	},
	start_upload: function(file){
		
		$(".submit").attr('disabled','disabled');
		if(!file) return false;
		var re = /\\/ig;
		if(re.test(file))
		{
			var parts = file.split("\\");
			file = parts[parts.length-1];
		}		
		
		var parts = file.split(".");
		var ext = parts[parts.length-1].toLowerCase();		
		if(this.permitted_ext.length > 0)
		{
			if(!in_array(ext,this.permitted_ext))
			{
				alert('Недопустимое расширение у файла '+file);
				return false;
			}
		}
		
		var re = /\//ig;
		if(re.test(file))
		{
			var parts = file.split("/");
			file = parts[parts.length-1];
		}
		
		var exists = false;
		
		// переписать 
		$('#uploads_list').find('span').each(function()
				{
					if($(this).html()==file) 
					{
						alert('Такой файл уже загружен!');
						exists = true;
					}
				});
		if(exists) return false;	
		this.uploaded_count++;
		
		if(this.uploaded_count>=this.max_uploads) { $('input[name=file]').fadeOut(); $('.not_uploaded').fadeOut(); }			
			
		var pos = this.frame_files[this.frames_list.length].length;
		this.frame_files[this.frames_list.length][pos] = this.total_files_count;
		
		$('#uploads_list').append('<div class="file_upload_progress" id="file' + this.total_files_count + '">' + file + '<a href="#" onclick="uploader.cancel(this); return false;"><span class="icon-cross float-right file_remove"></span></a><div class="progress"><div class="progress-bar progress-bar-striped bg-success" role="progressbar" aria-valuemax="100" aria-valuemin="0" aria-valuenow="0">0%</div></div></div>');

		this.active_uploads[this.total_files_count] = true;
		if(!this.timer) this.timer = setInterval(function(){uploader.check_state();},500);			
		this.total_files_count ++;
		return true;
	},	
	send: function()
	{		
		$('#upload_form').hide();
		var input = $('#upload_form').find('input[type=file]');
		if(input[0].files)
		{
			var files = input[0].files;
			var starts = false;
			for(i=0;i<files.length;i++)
			{
				var file = files[i];
				var is_started = this.start_upload(file.name);
				if(is_started) starts = true;
				else delete files[i];
			}
			if(!starts) return;
		}else
		{
			var file = input.val();
			var is_started = this.start_upload(file);
		}
		$('#upload_form').submit();
		this.add_upload();			
	},
	check_state: function() 
	{
		this.update_persents();
		if(this.pending) return;
		var count = 0;
		var url = uploader_url + "/progress/?";
		
		
			//up = up+1;
			
			for(ff in this.frame_files)
			{
				var flag = false;
				var frame = this.frame_files[ff];
				
				for(fid in frame)
				{
					if(this.active_uploads[frame[fid]]) flag = true;
				}
				
				if(flag) 
				{
					count++;
					url =uploader_url + '/progress/?X-Progress-ID='+this.master_key+ff;					
					
					$.ajax({
					    url: url,
					 		   
					    jsonp: "callback",
					 
					    dataType: "jsonp",
					 		 
					    data: {					       
					    },
					    
					    id : ff,
					    
					    success: function( response ) {
					    	uploader.pending = false;	
					    	
					    	var state = eval(response); 
					    	
					    	uploader.update_progress(state, this.id);
					    	 
					    }
					});
				}		 
			}			
		
		if(count<=0) 
		{			
			clearInterval(this.timer);
			this.timer = null;
			return;
		}
		
		this.pending = true;
					
		/*
		$.get(url,function(data)
				{			
					uploader.pending = false;					
					var state = eval('['+data+']'); 

					$.each(state, function(key, value) {						
						uploader.update_progress(value);
					});
				}); */
	},
	update_progress: function(val, id)
	{						
		/*for (var key in state) 
		{*/
			//var val = state [key];
			//var id = this.get_item_id(key);
			
			if(val)
			{							
				var size_for_one = parseInt(val.size)/this.frame_files[id].length				
				for(ff in this.frame_files[id])
				{
					var fid = this.frame_files[id][ff];
					this.size[fid] = size_for_one;
					this.check_size(fid);
				}												
				
				if(val['errors'] instanceof  Array)
				{
					// тут вывод ошибок								
					this.stop(id);				
					this.size[id] = 0;
					var error = new Array;
					for (var key in val['errors']) 
					{
						error[key] = val['errors'][key];
						this.show_error(error,id);
						return false;
					}				
				}else
				{
					//var pers = val['persent'];
					var pers = val.received / val.size  * 100;					
					//var est = val['est_sec']*1000;
					//est = est + 300;
					var total_width = parseInt($('.progress').css('width'), 10)
					
					for(ff in this.frame_files[id])
					{
						var fid = this.frame_files[id][ff];						
						$('#file'+fid).find('.progress-bar').css('width', pers+'%');						
						//$('#file'+fid).find('.progress-bar').stop().animate({'width': total_width+'px' },est,'linear');						
					}
				}
			}
			//$('#file'+id).find('.progress').animate({'width': pers+'px'},200);			
			//$('#file'+id).find('.persent').html(val['persent']+'%');		
		//}
	},
	update_persents: function()
	{
		$('.progress-bar').each(
		function() 
		{
			var total_width = parseInt($('.progress').css('width'), 10);
			var width = parseInt($(this).css('width'),10);
			var pers = Math.round(width*100 / total_width);		
			$(this).html(pers+'%');
		}		
		);
	},
	check_size: function(id)
	{
		var total = 0;
		var max_id = 0;
		for (var key in this.size) 
		{
			var files_file_id = this.files_file_id[key];
			var file_ = files.files[files_file_id];
			if(!files_file_id || (files_file_id && file_))
			total+=this.size[key]/1000000;
			max_id = key;
		}
		
		if(total>this.max_total_size && max_id == id) 
		{
			this.stop(id);
			this.show_error('Превышен максимальный суммарный объем.',id);
			return false;
		} return true;
	},
	check_count: function()
	{
		if(this.uploaded_count<this.max_uploads && !$('#add_attache').is(':visible')) 
		{
			$('input[name=file]').fadeIn(); $('.not_uploaded').fadeIn();
		}
	},
	stop: function(id)
	{
		this.uploaded_count--;
		this.check_count();
		delete this.active_uploads[id];
		delete this.size[id];
		var frame_id = '';
		for(frid in this.frame_files)
		{						
			var frame = this.frame_files[frid];
			for(fid in frame)
			{
				if(frame[fid]==id) 
				{
					frame_id = fid;
					break;
				}
			}			
			if(frame_id!='') break;
		}
				
		for(fid in this.frame_files[frid])
		{
			if(this.active_uploads[fid]) return; // во фрейме еще есть активные загрузки
		}
				
		var frames = window.frames;		
		for (var i = 0; i < frames.length; i++) 
		{
			//var fid=this.get_item_id(frames[i].name);
			if($(frames[i]).attr('name') == this.master_key+frid) 
			{
				//alert(frames[i]);
				//frames[i].stop();
				console.log('stopping '+frid+' frame');
				frames[i].location='about:blank';
				return;
			}
		}
	},
	get_item_id: function(str)
	{
		return str.replace(this.master_key,'');
	},
	loaded: function(item)
	{								
		try
		{
			console.log(item.contentWindow.location.href);
		} catch (err)
		{
			console.log('href error '+err);
			return;
		}
		
		var content = $(item).contents().find('body').html();			
		var id = this.get_item_id( $(item).attr('name'));
		
		//delete this.active_uploads[id];
		//var result = eval('['+content+']');
		//var result = result[0];
		var result = eval(content);
		if(result)	
		{
			var i = 0;
			for(res in result)
			{
				var data = result[res];
				var fid = this.frame_files[id][i];
				this.size[fid]= data['size'];
				delete this.active_uploads[fid];
				if(data['errors'])
				{
					// ошибки
					this.uploaded_count--;
					this.size[fid] = 0;
					this.check_count();
					for (var key in data['errors']) 
					{
						var error = data['errors'][key];				
						this.show_error(error,fid);
					}
				}else
				{				
				var parts = data.file_name.split(".");
				var ext = parts[parts.length-1].toLowerCase();							
				if(!this.check_size(fid) || (this.permitted_ext.length > 0 && !in_array(ext,this.permitted_ext))) 
					this.size[fid] = 0; 
				else 
					this.done(fid,data);
				}
				i++;
			}
		}
		
	},
	done: function(id,result)
	{
		$(".submit").removeAttr('disabled');
		this.show_error('',id);
		var name = $('#file'+id).find('span').html();		
		if(typeof(files)!='undefined')
		{
			$('#file'+id).remove();		
			files.add_file(result);
			this.files_file_id[id] = result.id;	
		}else
		{
			$('#file'+id).append('<input type="hidden" name="files[]" value="'+result['hash']+'"> <input type="hidden" name="filenames[]" value="'+name+'">');
		}
		if(typeof(this.loaded_callback)=='function') this.loaded_callback(result);
	},
	cancel: function(item)
	{
		$(".submit").removeAttr('disabled');
		var id = $(item).parent().attr('id');
		id = id.replace('file','');
		if(this.active_uploads[id]) this.stop(id);
		if($(item).parent().find('.error_').html()=='') { this.uploaded_count--; this.check_count();}
		$(item).parent().fadeOut(400);
		setTimeout(function(){$(item).parent().remove();},400);
		this.size[id] = 0;
		var fid = $(item).attr('fid');
		if(fid) 
		{
			var pid=$('input[name=id]').val();
			if(pid)
			{
				var url= '/projects/'+pid+'.html?action=delete_file&id='+fid;
				$.get(url,function(data){});
			}
			
			//if(typeof(this.delete_callback)=='function') this.delete_callback();
		}		
	},
	show_error: function(error,upload_id)
	{
		if(this.error_strings[error]) var error = this.error_strings[error];		
		
		if(error=='')
		{
			$('#file'+upload_id).find('.progress').stop().animate({'width': '100px'},400,'linear');
			setTimeout(function(){$('#file'+upload_id).find('.progress_container').remove();},400);
		} else
		{
			$('#file'+upload_id).find('.progress_container').remove();
		}
		$('#file'+upload_id).addClass('has-error').find('.form-control-feedback').remove();
		$('<span class="form-control-feedback">'+error+'</span>').appendTo('#file'+upload_id);		
		//$('#file'+upload_id).find('.cancel').html('<img src="/img/icons/12x12/delete.gif" alt="Удалить" title="Удалить">');
	}		
};

function in_array(needle, haystack, strict) {
  var found = false, key, strict = !!strict;	 
		    for (key in haystack) {
		        if ((strict && haystack[key] === needle) || (!strict && haystack[key] == needle)) {
		            found = true;
		            break;
		        }
		    }		 
		    return found;
}

$(function() {	
	
	if(uploader.ready) return;
	uploader.ready = true;			
	$('input[type=file].uploader').bind('change',function(){				
		$('#upload_form').find('input[type=file]').remove();		
		$(this).clone(true).val('').insertAfter(this);		
		$(this).insertAfter('#UPLOAD_IDENTIFIER');
		uploader.send();	
		return false;
	});

	$('.cancel').on('click',function(){ uploader.cancel(this); return false;});

	$('.attachment .link a').bind('click',function()
	{ 
		$(this).parent().hide();
		$(this).parent().parent().find('.file').fadeIn(400);		
		
		return false;
	});

	$(".filetypes").tooltip();
	
	uploader.add_upload();	
	
	file_manager_window = null;
	
	$('#open_file_manager').click(function(){
		
		var url = '/file_manager/?';
		
		if(typeof(portfolio)!='undefined')
		{
			for(i in portfolio.type)
			{
				url = url+'type[]='+portfolio.type[i]+'&';
			}
		}
		
		if(file_manager_window && !file_manager_window.closed)
		{
			file_manager_window.location = url;
		}else
		{
			file_manager_window = window.open(url,'File Manager', 'width=700, height=700, resizable=1, scrollbars=1, menubar=0, toolbar=0, titlebar=0');				
		}
		
		return false;
	});
	
	$('#save_video_btn').click(function(){
		var link = $('#video_url').val();
		link = encodeURIComponent(link);
		var url = '/ajax/?section=files&action=add_video_link&url='+link;
		$.get(url,
				function(data){
				var result = eval('['+data+']');
				var result = result[0];
				if(!result.errors)
				{
					files.add_file(result);
					$('#video_add_window').modal('hide');
				}else
				{
					alert(result.errors);
				}
		});
	});	
});var File = function(arg)
{
	if(typeof(arg)=='number')
	{
		this.get_exists(arg);
	}else this.add(arg);
};

File.prototype.get_exists = function (id)
{
	this.id = id;
	this.container = $('#file_'+id);
	var data = this.container.data('data');
	this.file_name = data.file_name;
	this.x = data.x;
	this.y = data.y;
	this.width = data.width;
	this.src_width = data.src_width;
	this.src_height = data.src_height;
	this.name = data.name;
	this.description = data.description;	
	this.preview = data.preview;
};

File.prototype.x = 0;
File.prototype.y = 0;
File.prototype.width = 500;

File.prototype.add = function (data)
{
	this.id = data.id;
	this.type = data.type;
	this.file_name = data.file_name;	
	this.x = data.x;
	this.y = data.y;
	this.width = data.width;
	this.src_width = data.src_width;
	this.src_height = data.src_height;
	this.name = data.name;
	this.description = data.description;
	this.preview = data.preview;
	this.container = $('#file_'+id);
};

File.prototype.edit = function ()
{
	
};

File.prototype.save = function ()
{
	
};

var File_controller = function(container)
{
	this.container = $(container);
	
	self_ = this;
	this.container.find('.work').each(function(i, item){
		self_.add_file($(item).data('data'));
	});
};

File_controller.prototype.files = [];
File_controller.prototype.actions = '';
File_controller.prototype.set_actions = function (actions)
{	
	this.actions = actions;
}

File_controller.prototype.add_file = function (data)
{
	if(typeof(data)!='object')
	{
		if(/file_\d+/.test(data))
		{
			id = data.replace(/file_/, '');			
		}else
		{
			id = data;
		}
		id = parseInt(id);
		var file = new File(id);
	}else
	{
		id = parseInt(data.id);
		var file = new File(data);
	}
	var container_id = '#file_'+id;
	
	if($(container_id).length==0)
	{		 
		var html = '<li class="work" id="file_' + id + '">'+
				'<input type="hidden" id="position_' + id + '">'+
				'<input type="hidden" name="files[]" value="' + id + '">'+
				'<div>'+
					'<img src="' + data.preview + '" alt="" id="file_preview_' + id + '">'+
				'</div>'+
				'<div class="work-title">' + data.file_name + '</div>'+
				'<div class="controls">'+
				  this.actions+
					'<a class="icon-bin delete" href="" title="Удалить"></a>'+
				'</div>'+
			'</li>';
				
		
		this.container.prepend(html);
		$(container_id).data('data',data);
	}
	
	this.files[file.id] = file; 
	self_.check_positions();
	this.container.trigger('file.file_added');
};

File_controller.prototype.file_saved = function()
{	
	if(!this.current_file) return;	
		$('#file_edit_window').modal('hide');
		var file = this.files[this.current_file];
		var img = $('#file_preview_'+file.id);
		var src = img.attr('src');
		
		var time = new Date();				
		$('#file_preview_'+file.id).attr('src',src+'?'+time);
};

File_controller.prototype.save_file_preview = function ()
{
	var file = this.files[this.current_file];	
	file.x = $('#x').val();
	file.y = $('#y').val();
	file.width = $('#width').val();

	this.current_file = file.id;
	$('#save_file_data').submit();		
};

File_controller.prototype.save_file_descr = function ()
{
	$('#file_description').sync();
	var file = this.files[this.current_file];	
	file.description = $('#file_description').val();
	file.name = $('#file_name').val();
	var url = '/ajax/?section=files&action=save_file_descr&id='+file.id;
	var data = {'description':file.description,'name':file.name};
	$.post(url, data,
			function(data){ 
			$('#file_description').destroy();
			$('#file_edit_descr_window').modal('hide');
	});
};

File_controller.prototype.check_positions = function(){
	$('#files_list .work').each(function(i,item){
		var dat = $(item).data('data');
		var id = dat.id;
		$('#position_'+id).attr('name','files_position['+id+']').val(i);
	});
};	

File_controller.prototype.edit_file_descr = function (item)
{
	var data = $(item).parents('.work').data('data');			
	var file = this.files[data.id];
	$('#file_description').val(file.description);
	$('#file_name').val(file.name);
	$('#file_edit_descr_window').modal('show');

	var wbbOpt = 
	{
		buttons: "bold,italic,underline,strike,|,bullist,|,h2,h3",
		allButtons: {
			bullist: {
				transform: {
					'<ul>{SELTEXT}</ul>':'[list]{SELTEXT}[/list]',
					'<li>{SELTEXT}</li>':'[*]{SELTEXT[^\[\]\*]}',
				}
			},
			h2:	{
				title: 'Заголовок H2',
				buttonText: 'H2',
				transform: {
					'<div class="h2">{SELTEXT}</div>':'[h2]{SELTEXT}[/h2]'
				}
			},
			h3:	{
				title: 'Заголовок H3',
				buttonText: 'H3',
				transform: {
					'<div class="h3">{SELTEXT}</div>':'[h3]{SELTEXT}[/h3]'
				}
			}
		}, 
		resize_maxheight: 500,
		minheight: 200
	};
	$('#file_description').wysibb(wbbOpt);
	$('#file_description').bbcode(file.description);
	this.current_file = file.id;
};

File_controller.prototype.edit_file_preview = function (item)
{
	var data = $(item).parents('.work').data('data');			
	var file = this.files[data.id];
	
	//$('#file_preview').attr('src','/download/'+file.id+'_500xp.jpg');
	//$('#file_description').val(file.description);
	$('#fid').val(file.id);
	
	
	x = file.x; 
	y = file.y;
	x2 = parseInt(file.x)+parseInt(file.width);
	y2 = parseInt(file.y)+((parseInt(file.width)/4)*3);	
	
	mult = file.src_width/500;
	minH = 187;
	minW = 250;
	
	$('#small_preview').attr('src',files_url+'/download/'+file.id+'_500xp.jpg');	
	$('#file_preview').hide();	
	
	$('#file_preview').on('load',function(){
		if(!file.loaded)
		{
			files.files[file.id].loaded = true;			
			jcrop_api.setImage(files_url+'/download/'+file.id+'_500xp.jpg');
			jcrop_api.setOptions({trueSize: [file.src_width, file.src_height]});
			if(file.width>0)
			{
				setTimeout(function(){
						jcrop_api.setSelect([ x, y, x2, y2 ]); 
						$('#file_preview').show();								
					},200);
			}else
			{
				jcrop_api.release(); 
				$('#file_preview').show();				
			}
			
			$('#file_edit_window').modal('show');
		}
	});	
	$('#file_preview').attr('src',files_url+'/download/'+file.id+'_500xp.jpg');	
	
	jcrop_api.setOptions({trueSize: [file.src_width, file.src_height]});
	
	
	if(file.loaded)
	{
		jcrop_api.setImage(files_url+'/download/'+file.id+'_500xp.jpg');	
		if(file.width==0)
		{
			setTimeout(function(){jcrop_api.release();},200);
		}else
		{
			setTimeout(function(){
				jcrop_api.setSelect([ x, y, x2, y2 ]);},200);
		}
		$('#file_preview').show();		
		$('#file_edit_window').modal('show');
	}
	this.current_file = file.id;
};

File_controller.prototype.delete_file = function (item)
{
	var data = $(item).parents('.work').data('data');			
	var file = this.files[data.id];
	$('#file_'+file.id).remove();
	delete(this.files[file.id]);	
	this.container.trigger('file.file_deleted');
};

function selected(c)
{
	$('#x').val(c.x);
	$('#y').val(c.y);
	$('#width').val(c.w);
	
	var rx = 250 / c.w;
	var ry = 187 / c.h;

	wi = $('.jcrop-holder').width()* mult;
	he = $('.jcrop-holder').height()* mult;
	$('#small_preview').css({
		width: Math.round(rx * wi) + 'px',
		height: Math.round(ry * he) + 'px',
		marginLeft: '-' + Math.round(rx * c.x) + 'px',
		marginTop: '-' + Math.round(ry * c.y) + 'px'
	});
}

$(function(){
	
	files = new File_controller('#files_list');
	mult = 1;
	
	if(typeof($('#file_preview').Jcrop)!='undefined' && typeof(jcrop_api)=='undefined')
	{	
		jcrop_api = $.Jcrop($('#file_preview'),{
			onSelect : selected,				
			onChange : selected,
			minSize: [250,187],
			aspectRatio: 4/3,
			setSelect:   [ 0, 0, 500, 500 ]
		});
		if(typeof(jcrop_api)!='undefined') jcrop_api.release();
	}
	
	$(window).trigger('files_loaded');

	$('.work_list').delegate('.work .icon-cog','click',function(){
		files.edit_file_preview(this);	
		files.edit_file_descr(this);
		return false;
	});
	
	$('.files_list').delegate('.work .edit_descr','click',function(){
		files.edit_file_descr(this);
		return false;
	});	
	
	$('#close_overlay_descr').click(function(){
		setTimeout(function(){ $('#file_description').destroy();},300);
	});
	
	$('.files_list').delegate('.work .delete','click',function(){
		files.delete_file(this);
		return false;
	});
	
	$('#save_file_data_btn').click(function(){
		files.save_file_preview();
		files.save_file_descr();
	});	
	
	$('#frame').on('load',function(){
		files.file_saved();
	});
	
	$('#file_preview').on('load',function(){jcrop_api.setSelect([ x, y, x2, y2 ]);});
	
	$( "#files_list" ).sortable(
			{
				stop: function( event, ui ) { // пїЅпїЅпїЅпїЅпїЅпїЅпїЅпїЅпїЅпїЅпїЅ 
									files.check_positions();
											}
			}		
	);
	$( "#files_list" ).disableSelection();	
	
	files.check_positions();
});/**
